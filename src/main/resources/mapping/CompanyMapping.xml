<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bugmaker.mapper.CompanyMapper">

   

	<insert id="insertCompany" parameterType="com.bugmaker.bean.Company">
	  insert into company values (#{id},#{name},#{address},#{phone},#{advisor})
	</insert>
	
	<select id="getAllCompany" resultType="Company">
	   select * from company
	</select>
	
</mapper>